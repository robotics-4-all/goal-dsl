TimeConstraint:
    type=TimeConstraintType '(' (comparator=ArithmeticComparator)? time=FLOAT ')'
;

ArithmeticComparator: '>' | '<' | '==' | '>=' | '<=';

TimeConstraintType:
    'FROM_SCENARIO_START'   |
    'FROM_GOAL_START'       |
    'FOR_TIME'              |
    'BETWEEN_GOALS_MIN'     |
    'BETWEEN_GOALS_MAX'
;
